    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@types/jest": "^27.0.3",
    "firebase": "^9.5.0",
    "jest": "^27.4.2",
    "jest-junit": "^13.0.0",
    "uuid": "^8.3.2"
